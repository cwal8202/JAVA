import java.util.Scanner;

public class Reverse{
	public static void main(String args[]){
		Scanner scan = new Scanner(System.in);
		System.out.print("숫자 입력하세요 : ");
		int a = scan.nextInt();
		int b = a/10000;
		int c = a%10000/1000;
		int d = a%1000/100;
		int e = a%100/10;
		int f = a%10;
		
		System.out.print(f + e + d +c + b + "");
	}
}