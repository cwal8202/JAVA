// 사용자의 입력 n, m에 대해
// n^m(n의 m제곱)을 출력하기
import java.util.Scanner;

public class Power {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		System.out.println("n 값 과 m값을 입력하세요 (n ^ m) : ");
		System.out.print("n 값 : ");
		int n = scan.nextInt();
		System.out.print("m 값 : ");
		int m = scan.nextInt();
		int result = 1;
		for (int i = 1; i <= m; i++) {
			result *= n;
		}
		System.out.println("값은 = " + result);
	}
}
